{
  "caption": "Database",
  "description": "Simplified database object for OCSF Lite.",
  "extends": "object",
  "name": "database",
  "attributes": {
    "desc": {
      "description": "The description of the database.",
      "requirement": "optional",
      "type": "string_t"
    },
    "modified_time": {
      "description": "The most recent time when any changes were made within the database.",
      "requirement": "optional",
      "type": "timestamp_t"
    },
    "name": {
      "description": "The database name.",
      "requirement": "recommended",
      "type": "string_t"
    },
    "type": {
      "description": "The database type.",
      "requirement": "optional",
      "type": "string_t"
    },
    "type_id": {
      "description": "The database type identifier.",
      "requirement": "optional",
      "type": "integer_t"
    },
    "uid": {
      "description": "The unique identifier of the database.",
      "requirement": "recommended",
      "type": "string_t"
    }
  }
}