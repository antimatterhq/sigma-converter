{
  "caption": "CVSS Score",
  "description": "Simplified CVSS scoring object for OCSF Lite.",
  "extends": "object",
  "name": "cvss",
  "attributes": {
    "base_score": {
      "description": "The CVSS base score.",
      "requirement": "required",
      "type": "float_t"
    },
    "overall_score": {
      "description": "The CVSS overall score.",
      "requirement": "recommended",
      "type": "float_t"
    },
    "severity": {
      "description": "The severity rating.",
      "requirement": "optional",
      "type": "string_t"
    },
    "src_url": {
      "description": "The source URL for the CVSS score.",
      "requirement": "optional",
      "type": "string_t"
    }
  }
}