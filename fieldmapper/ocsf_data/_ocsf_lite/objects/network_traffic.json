{
  "caption": "Network Traffic",
  "description": "The Network Traffic object describes characteristics of network traffic. Network traffic refers to data moving across a network at a given point of time.",
  "extends": "object",
  "name": "network_traffic",
  "attributes": {
    "bytes": {
      "requirement": "recommended",
      "type": "long_t"
    },
    "bytes_in": {
      "requirement": "optional",
      "type": "long_t"
    },
    "bytes_missed": {
      "requirement": "optional",
      "type": "long_t"
    },
    "bytes_out": {
      "requirement": "optional",
      "type": "long_t"
    },
    "chunks": {
      "description": "The total number of chunks (in and out).",
      "requirement": "optional",
      "type": "long_t"
    },
    "chunks_in": {
      "description": "The number of chunks sent from the destination to the source.",
      "requirement": "optional",
      "type": "long_t"
    },
    "chunks_out": {
      "description": "The number of chunks sent from the source to the destination.",
      "requirement": "optional",
      "type": "long_t"
    },
    "packets": {
      "requirement": "recommended",
      "type": "long_t"
    },
    "packets_in": {
      "requirement": "optional",
      "type": "long_t"
    },
    "packets_out": {
      "requirement": "optional",
      "type": "long_t"
    }
  },
