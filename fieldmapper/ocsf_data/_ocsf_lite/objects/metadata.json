{
  "caption": "Metadata",
  "description": "Simplified metadata object for lite schema.",
  "extends": "object",
  "name": "metadata",
  "attributes": {
    "correlation_uid": {
      "requirement": "optional",
      "type": "string_t"
    },
    "event_code": {
      "requirement": "optional",
      "type": "string_t"
    },
    "log_level": {
      "requirement": "optional",
      "type": "string_t"
    },
    "log_name": {
      "requirement": "recommended",
      "type": "string_t"
    },
    "log_provider": {
      "requirement": "recommended",
      "type": "string_t"
    },
    "log_version": {
      "requirement": "optional",
      "type": "string_t"
    },
    "logged_time": {
      "requirement": "optional",
      "type": "timestamp_t"
    },
    "modified_time": {
      "description": "The time when the event was last modified or enriched.",
      "requirement": "optional",
      "type": "timestamp_t"
    },
    "original_time": {
      "requirement": "recommended",
      "type": "string_t"
    },
    "processed_time": {
      "requirement": "optional",
      "type": "timestamp_t"
    },
    "product": {
      "requirement": "required"
    },
    "tags": {
      "description": "The list of tags associated to the event.",
      "requirement": "optional",
      "type": "json_t"
    },
    "tenant_uid": {
      "requirement": "recommended",
      "type": "string_t"
    },
    "uid": {
      "caption": "Event UID",
      "description": "The logging system-assigned unique identifier of an event instance.",
      "requirement": "optional",
      "type": "string_t"
    },
    "version": {
      "description": "The version of the OCSF schema.",
      "requirement": "required",
      "type": "string_t"
    }
  }
}