{
  "caption": "HTTP Request",
  "description": "Simplified HTTP Request object for OCSF Lite.",
  "extends": "object",
  "name": "http_request",
  "attributes": {
    "args": {
      "description": "The HTTP request arguments.",
      "requirement": "optional",
      "type": "string_t"
    },
    "body_length": {
      "description": "The actual length of the HTTP request body, in number of bytes.",
      "requirement": "optional",
      "type": "integer_t"
    },
    "http_headers": {
      "description": "The HTTP request headers.",
      "requirement": "optional",
      "is_array": true
    },
    "http_method": {
      "description": "The HTTP request method.",
      "requirement": "optional",
      "type": "string_t"
    },
    "length": {
      "description": "The length of the HTTP request.",
      "requirement": "optional",
      "type": "integer_t"
    },
    "referrer": {
      "description": "The HTTP referrer.",
      "requirement": "optional",
      "type": "string_t"
    },
    "url": {
      "description": "The requested URL.",
      "requirement": "optional",
      "type": "string_t"
    },
    "user_agent": {
      "description": "The HTTP user agent.",
      "requirement": "optional",
      "type": "string_t"
    },
    "version": {
      "description": "The HTTP version.",
      "requirement": "optional",
      "type": "string_t"
    }
  }
}