{
  "observable": 25,
  "caption": "Process",
  "description": "Simplified Process object for lite schema.",
  "extends": "object",
  "name": "process",
  "attributes": {
    "cmd_line": {
      "description": "The full command line used to launch the process.",
      "requirement": "recommended",
      "type": "string_t"
    },
    "cpid": {
      "description": "The parent process identifier.",
      "requirement": "optional",
      "type": "string_t"
    },
    "name": {
      "description": "The friendly name of the process.",
      "requirement": "recommended",
      "type": "string_t"
    },
    "pid": {
      "description": "The process identifier.",
      "requirement": "recommended",
      "type": "integer_t"
    },
    "session": {
      "description": "The user session context.",
      "requirement": "optional"
    },
    "uid": {
      "description": "The unique process identifier.",
      "requirement": "recommended",
      "type": "string_t"
    },
    "user": {
      "description": "The user associated with the process.",
      "requirement": "optional"
    }
  }
}