{
  "metadata": {
    "name": "Suspicious HH.EXE Execution",
    "comment": "Detects a suspicious execution of a Microsoft HTML Help (HH.exe)",
    "annotations": {
      "source": "sigma",
      "category": "process_creation",
      "product": "windows",
      "sigma_status": "test"
    }
  },
  "spec": {
    "schedule": {
      "atLeastEvery": "15m",
      "enabled": true
    },
    "input": {
      "batch": {
        "sql": "SELECT * FROM process_activity WHERE time BETWEEN CURRENT_TIMESTAMP() - INTERVAL 24 HOUR AND CURRENT_TIMESTAMP() AND (lower(activity_id) = lower('1') AND lower(process.file.name) = lower('hh.exe') AND endswith(lower(process.name), lower('\\\\hh.exe')) AND (contains(lower(process.cmd_line), lower('.application')) OR contains(lower(process.cmd_line), lower('\\\\appdata\\\\local\\\\temp\\\\')) OR contains(lower(process.cmd_line), lower('\\\\content.outlook\\\\')) OR contains(lower(process.cmd_line), lower('\\\\downloads\\\\')) OR contains(lower(process.cmd_line), lower('\\\\users\\\\public\\\\')) OR contains(lower(process.cmd_line), lower('\\\\windows\\\\temp\\\\'))))"
      }
    },
    "output": {
      "summary": "Detects a suspicious execution of a Microsoft HTML Help (HH.exe)",
      "defaultContext": true
    },
    "metadata": {
      "version": 1.0,
      "severity": "High",
      "fidelity": "Investigative",
      "objective": "False positive: Unknown References: https://www.trustwave.com/en-us/resources/blogs/spiderlabs-blog/chm-badness-delivers-a-banking-trojan/, https://github.com/elastic/protections-artifacts/commit/746086721fd385d9f5c6647cada1788db4aea95f#diff-27939090904026cc396b0b629c8e4314acd6f5dac40a676edbc87f4567b47eb7, https://www.ptsecurity.com/ww-en/analytics/pt-esc-threat-intelligence/higaisa-or-winnti-apt-41-backdoors-old-and-new/, https://www.zscaler.com/blogs/security-research/unintentional-leak-glimpse-attack-vectors-apt37",
