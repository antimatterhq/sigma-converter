{
  "metadata": {
    "name": "Network Connection Initiated By Eqnedt32.EXE",
    "comment": "Detects network connections from the Equation Editor process \"eqnedt32.exe\".",
    "annotations": {
      "source": "sigma",
      "category": "network_connection",
      "product": "windows",
      "sigma_status": "test"
    }
  },
  "spec": {
    "schedule": {
      "atLeastEvery": "12h",
      "enabled": false
    },
    "input": {
      "batch": {
        "sql": "SELECT * FROM process_activity WHERE time BETWEEN CURRENT_TIMESTAMP() - INTERVAL 24 HOUR AND CURRENT_TIMESTAMP() AND (endswith(lower(process.name), lower('\\\\\\\\eqnedt32.exe')))"
      }
    },
    "output": {
      "summary": "Detects network connections from the Equation Editor process \"eqnedt32.exe\".",
      "defaultContext": true
    },
    "metadata": {
      "version": 1.0,
      "severity": "High",
      "fidelity": "Investigative",
