  "metadata": {
    "name": "OpenWith.exe Executes Specified Binary",
    "comment": "The OpenWith.exe executes other binary",
    "annotations": {
      "source": "sigma",
      "category": "process_creation",
      "product": "windows",
      "sigma_status": "test"
    }
  },
  "spec": {
    "schedule": {
      "atLeastEvery": "15m",
      "enabled": true
    },
    "input": {
      "batch": {
        "sql": "SELECT * FROM process_activity WHERE time BETWEEN CURRENT_TIMESTAMP() - INTERVAL 24 HOUR AND CURRENT_TIMESTAMP() AND (lower(activity_id) = lower('1') AND endswith(lower(process.name), lower('\\\\openwith.exe')) AND contains(lower(process.cmd_line), lower('/c')))"
      }
    },
    "output": {
      "summary": "The OpenWith.exe executes other binary",
      "defaultContext": true
    },
