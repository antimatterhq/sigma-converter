{
  "metadata": {
    "name": "Octopus Scanner Malware",
    "comment": "Detects Octopus Scanner Malware.",
    "annotations": {
      "source": "sigma",
      "category": "file_event",
      "product": "windows",
      "sigma_status": "test"
    }
  },
  "spec": {
    "schedule": {
      "atLeastEvery": "12h",
      "enabled": false
    },
    "input": {
      "batch": {
        "sql": "SELECT * FROM file_activity WHERE time BETWEEN CURRENT_TIMESTAMP() - INTERVAL 24 HOUR AND CURRENT_TIMESTAMP() AND (activity_id = 3 AND (endswith(lower(file.path), lower('\\\\\\\\appdata\\\\\\\\local\\\\\\\\microsoft\\\\\\\\cache134.dat')) OR endswith(lower(file.path), lower('\\\\\\\\appdata\\\\\\\\local\\\\\\\\microsoft\\\\\\\\explorersync.db'))))"
      }
    },
    "output": {
      "summary": "Detects Octopus Scanner Malware.",
      "defaultContext": true
    },
    "metadata": {
      "version": 1.0,
      "severity": "High",
      "fidelity": "Investigative",
      "objective": "False positive: Unknown Reference: https://securitylab.github.com/research/octopus-scanner-malware-open-source-supply-chain",
      "mitre": [
        {
          "taxonomy": "MITRE ATT&CK",
          "techniqueId": "T1195"
        },
        {
          "taxonomy": "MITRE ATT&CK",
          "techniqueId": "T1195",
          "subTechniqueId": "001"
        }
      ]
    },
    "computeMode": "high"
  },
  "apiVersion": "v1",
  "kind": "Rule"
}